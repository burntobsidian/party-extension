{
  "manifest_version": 3,
  "name": "Party Members",
  "description": "An extension that shows other party members on the same page.",
  "version": "1.0",
  "action": {
    "default_icon": {
      "16": "images/party-balloons-16.png",
      "32": "images/party-balloons-32.png",
      "48": "images/party-balloons-48.png"
    }
  },
  "permissions": ["activeTab", "tabs"],
  "icons": {
    "16": "images/party-balloons-16.png",
    "32": "images/party-balloons-32.png",
    "48": "images/party-balloons-48.png",
    "128": "images/party-balloons-128.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
